{
  "name": "redux-fractal",
  "version": "1.0.0",
  "description": "A local component state management library using redux",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "$(npm bin)/babel -d dist src",
    "prepublish": "$(npm bin)/babel -d dist src",
    "test": "ava --watch tests/*Spec.js"
  },
  "ava": {
    "require": [
      "babel-register",
      "./tests/helpers/setupEnv.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcazaciuc/redux-fractal.git"
  },
  "keywords": [
    "redux",
    "local",
    "component",
    "state",
    "fractal",
    "architecture",
    "ui",
    "state"
  ],
  "author": "Cazaciuc Gabriel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gcazaciuc/redux-fractal/issues"
  },
  "homepage": "https://github.com/gcazaciuc/redux-fractal#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "enzyme": "^2.4.1",
    "jsdom": "^9.4.2",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1"
  },
  "dependencies": {
    "invariant": "^2.2.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
